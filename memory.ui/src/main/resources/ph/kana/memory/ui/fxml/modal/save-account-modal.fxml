<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root type="TilePane" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml" alignment="CENTER" prefHeight="200.0" prefWidth="200.0" styleClass="modal-darkening">
<children>
    <AnchorPane prefHeight="280.0" prefWidth="280.0" styleClass="modal">
        <children>
            <Label fx:id="titleLabel" layoutX="14.0" layoutY="14.0" styleClass="title" text="Add Account" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0" />
            <Label layoutX="15.0" layoutY="85.0" text="Domain" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="60.0" />
            <TextField fx:id="domainTextBox" layoutX="15.0" layoutY="76.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="80.0" />
            <Label layoutX="14.0" layoutY="116.0" text="Username" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="115.0" />
            <TextField fx:id="usernameTextBox" layoutX="14.0" layoutY="136.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="135.0" />
            <Label layoutX="23.0" layoutY="188.0" text="Password" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="170.0" />
            <PasswordField fx:id="maskedPasswordTextBox" layoutX="30.0" layoutY="233.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="190.0" />
            <TextField fx:id="unmaskedPasswordTextBox" layoutX="30.0" layoutY="233.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="190.0" />
            <CheckBox fx:id="maskPasswordToggle" layoutX="185.0" layoutY="169.0" mnemonicParsing="false" text="Show?" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="170.0" />
            <Button cancelButton="true" layoutX="204.0" layoutY="335.0" mnemonicParsing="false" onAction="#close" text="Cancel" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="240.0" />
            <Button defaultButton="true" layoutX="146.0" layoutY="240.0" mnemonicParsing="false" onAction="#saveAccount" text="Save" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="85.0" AnchorPane.topAnchor="240.0" />
        </children></AnchorPane>
</children>
</fx:root>
